<% 
filterStatusHelper = (linkPrefix,statusFilter,keyword) => {
    xtmlResult = "";
    statusFilter.forEach((item) =>{ 
        let link = linkPrefix + 'status/' + item.name;
        if(keyword !== "" && keyword !== undefined) link += '?keyword=' + keyword;

        xtmlResult += `<a class="btn m-b-sm btn-${item.class} btn-sm" href="${link}">
            ${item.name.toUpperCase() + "(" +item.count + ")"}
        </a> `
    })
    return xtmlResult;
}
%>
